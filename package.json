{
  "name": "portfolio-next",
  "description": "My Portfolio Site",
  "author": {
    "email": "coreyburns@outlook.com",
    "name": "Corey Burns",
    "url": "https://coreyburns.dev"
  },
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://https://github.com/corey-burns-dev/portfolio-next"
  },
  "license": "MIT",
  "packageManager": "bun@1.3.1",
  "keywords": [
    "gatsby",
    "typescript",
    "nextjs",
    "portfolio",
    "frontend",
    "backend",
    "react",
    "graphql",
    "styled-components",
    "full-stack",
    "emotion",
    "chakra-ui",
    "styled-system"
  ],
  "scripts": {
    "dev": "next",
    "build": "npx @cloudflare/next-on-pages",
    "build-export": "cross-env NEXT_OUTPUT_MODE=export next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "bun run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "start": "next start",
    "analyze": "ANALYZE=true next build --webpack",
    "find:unused": "next-unused",
    "type-check": "tsc --pretty --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --files"
    ]
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.29.0",
    "@chakra-ui/theme-tools": "2.2.6",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@next/bundle-analyzer": "^16.0.1",
    "framer-motion": "^12.23.24",
    "next": "^16.0.1",
    "next-unused": "^0.0.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-intersection-observer": "^10.0.0",
    "react-scroll": "^1.9.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-scroll": "^1.8.10",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "@biomejs/biome": "^2.3.4",
    "typescript": "5.9.3",
    "wrangler": "^3.94.0"
  },
  "next-unused": {
    "alias": {
      "@common/*": [
        "components/common/*"
      ],
      "@sections/*": [
        "components/sections/*"
      ],
      "@ui/*": [
        "components/ui/*"
      ],
      "@configs/*": [
        "configs/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "configs",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
